{
  "semi": false,
  "tabWidth": 2,
  "printWidth": 120,

  "useTabs": false,
  "singleQuote": true,
  "insertPragma": false,
  "bracketSpacing": true,
  "requirePragma": false,
  "jsxSingleQuote": false,
  "bracketSameLine": false,

  "htmlWhitespaceSensitivity": "strict",
  "quoteProps": "as-needed",
  "arrowParens": "always",
  "trailingComma": "es5",
  "proseWrap": "never",
  "endOfLine": "lf"
}
